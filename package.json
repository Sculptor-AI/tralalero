{
  "name": "tralalero",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "pages:dev": "wrangler pages dev --local --persist ./dist",
    "pages:build": "npm run build",
    "pages:deploy": "npm run build && wrangler pages deploy ./dist",
    "db:init": "wrangler d1 execute tralalero-db --local --file=./schema.sql",
    "db:init:remote": "wrangler d1 execute tralalero-db --file=./schema.sql"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "vite": "^5.4.10",
    "wrangler": "^3.0.0"
  }
}
